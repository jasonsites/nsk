  "check-coverage": true,
  "exclude": ["node_modules/"],
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": [".ts"],
  "include": ["src/**/*.ts"],
  "lines": 0,
  "report-dir": "./test/coverage",
  "reporter": ["lcov", "text-summary"],
  "temp-directory": "./test/coverage/output"
}
