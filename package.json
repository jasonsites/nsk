{
  "name": "nsk-api",
  "version": "0.0.1",
  "description": "Node Starter Kit for HTTP API Applications",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.15.0"
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rm -rf dist/",
    "coverage": "nyc npm t",
    "dev": "nodemon -r tsconfig-paths/register",
    "migrate": "npm run build && node dist/index.js migrate",
    "migrate:make": "node database/make.js",
    "lint": "eslint src test --ext .ts,.js",
    "release": "echo 'replace with `npm run build && npx standard-version` after v1.0.0'",
    "start": "node dist/index.js",
    "test": "mocha 'test/tests/**/*.test.ts'"
  },
  "keywords": [
    "api",
    "http",
    "koa",
    "node",
    "starter kit",
    "typescript",
    "web"
  ],
  "author": "Jason Sites <email@jasonsites.com> (https://github.com/jasonsites)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsites/nsk-api.git"
  },
  "bugs": {
    "url": "https://github.com/jasonsites/nsk-api/issues"
  },
  "homepage": "https://github.com/jasonsites/nsk-api#readme",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "app-container": "^1.1.2",
    "axios": "^1.4.0",
    "bunyan": "^1.8.15",
    "config": "^3.3.9",
    "joi": "^17.9.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-compress": "^5.1.1",
    "koa-helmet": "^7.0.2",
    "koa-router": "^12.0.0",
    "kysely": "^0.24.2",
    "pg": "^8.11.0",
    "qs": "^6.11.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/bluebird": "^3.5.38",
    "@types/boom": "^7.3.2",
    "@types/bunyan": "^1.8.8",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-uuid": "^1.0.2",
    "@types/chance": "^1.1.3",
    "@types/config": "^3.3.0",
    "@types/koa": "^2.13.6",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.194",
    "@types/lodash.assignin": "^4.2.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.3",
    "@types/pg": "^8.10.0",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "bluebird": "^3.7.2",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-uuid": "^1.0.6",
    "chance": "^1.1.11",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "lodash.assignin": "^4.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "pirates": "^4.0.5",
    "sinon": "^15.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
