{
  "api": {
    "baseURL": "API_BASEURL",
    "port": "API_PORT"
  },
  "logger": {
    "app": {
      "enabled": "LOGGER_APP_ENABLED",
      "level": "LOGGER_APP_LEVEL"
    },
    "http": {
      "request": {
        "enabled": "LOGGER_HTTP_REQUEST_ENABLED",
        "level": "LOGGER_REPO_LEVEL"
      },
      "response": {
        "enabled": "LOGGER_HTTP_RESPONSE_ENABLED",
        "level": "LOGGER_HTTP_LEVEL"
      }
    },
    "repo": {
      "enabled": "LOGGER_REPO_ENABLED",
      "level": "LOGGER_REPO_LEVEL"
    },
    "services": {
      "example": {
        "enabled": "LOGGER_SVC_EXAMPLE_ENABLED",
        "level": "LOGGER_SVC_EXAMPLE_LEVEL"
      }
    },
    "validation": {
      "enabled": "LOGGER_VALIDATION_ENABLED",
      "level": "LOGGER_VALIDATION_LEVEL"
    }
  },
  "postgres": {
    "options": {
      "connection": {
        "database": "POSTGRES_DB",
        "host": "POSTGRES_HOST",
        "password": "POSTGRES_PASSWORD",
        "port": "POSTGRES_PORT",
        "user": "POSTGRES_USER"
      },
      "pool": {
        "max": "PGCONN_POOL_MAX",
        "min": "PGCONN_POOL_MIN"
      },
      "version": "POSTGRES_VERSION"
    }
  },
  "services": {
    "example": {
      "api": {
        "baseURL": "SVC_EXAMPLE_BASEURL",
        "headers": "SVC_EXAMPLE_HEADERS",
        "timeout": "SVC_EXAMPLE_TIMEOUT"
      }
    }
  }
}
